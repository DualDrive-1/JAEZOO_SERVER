{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${POSTGRES_HOST};Port=${POSTGRES_PORT};Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};Ssl Mode=Require;Trust Server Certificate=true"
  },
  "Jwt": {
    "Issuer": "JaeZoo",
    "Audience": "JaeZooClient",
    "Key": "${JWT__KEY}",
    "ExpiresMinutes": 60
  },
  "AllowedOrigin": "http://localhost:5173",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  }
}
